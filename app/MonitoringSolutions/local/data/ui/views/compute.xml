    <panel>
      <title>Disk Write</title>
      <chart>
        <title>MBps</title>
        <search>
          <query>type=collectd sourcetype=server_stats Hostname=$compute$ JobTagName=$JobTagName$ plugin=readdisk collectd_type=disk_octets | stats values(write) as valueWrite by _time,Hostname | streamstats current=f window=1 last(valueWrite) as oldWrite last(_time) as oldtime | eval total = (valueWrite-oldWrite)/(_time-oldtime) | eval disk_write=(total/1000000) | timechart avg(disk_write) as "Disk Write (MBps)" by Hostname</query>
          <earliest>$time_tok.earliest$</earliest>
