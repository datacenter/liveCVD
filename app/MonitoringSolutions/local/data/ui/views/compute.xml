  <row>
    <panel>
      <title>TCP Resets</title>
      <chart>
        <search>
          <query>type=collectd sourcetype=server_stats Hostname=$compute$ JobTagName=$JobTagName$ plugin=protocols | stats values(TCP_ResetSegs) as tcp by _time,Hostname | streamstats current=f window=1 last(tcp) as old_tcp| eval TCP_ResetSegs = (tcp-old_tcp) | timechart avg(TCP_ResetSegs) as "TCP Resets" by Hostname</query>
          <earliest>$time_tok.earliest$</earliest>
