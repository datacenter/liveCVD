<form>
 <label>Switch Time Series</label>
  <fieldset submitButton="false" autoRun="true">
    <input type="dropdown" token="switch_name">
       <label>Select Switch</label>    
      <search>
             <query>type=switch sourcetype=server_stats plugin=interface JobTagName=$JobTagName$ | stats values(subtype) as Switch_Name by subtype | table Switch_Name</query>
           </search>
           <fieldForLabel>Switch_Name</fieldForLabel>
           <fieldForValue>Switch_Name</fieldForValue>
      
    </input>
    <input type="dropdown" token="instance_name">
        <label>Select Interface</label>   
      <search>
             <query>type=switch sourcetype=server_stats subtype=$switch_name$ plugin=interface JobTagName=$JobTagName$ | stats values(instance) as Instance_Name by subtype,instance | table Instance_Name</query>
           </search>
           <fieldForLabel>Instance_Name</fieldForLabel>
           <fieldForValue>Instance_Name</fieldForValue>
      <choice value="*">All</choice>
           
    </input>
    <input type="time" token="time_tok" searchWhenChanged="true">
      <label>Select time</label>
      <default>
        <earliest>0</earliest>
        <latest></latest>
      </default>
    </input>
  </fieldset>
  <row>
    <panel>
      <title>Rx</title>
      <chart>
        <title>Mbps</title>
        <search>
          <query>type=switch sourcetype=server_stats subtype=$switch_name$ instance=$instance_name$ plugin= interface JobTagName=$JobTagName$ | stats values(eth_inbytes) as valueRx by instance, _time |streamstats current=f window=1 last(valueRx) as oldRx last(_time) as oldtime by instance | eval total = (valueRx-oldRx)/(_time-oldtime) |eval Rx_Rate=(total*8)/1000000 | timechart avg(Rx_Rate) as "Rx (Mbps)" by instance</query>
          <earliest>$time_tok.earliest$</earliest>
          <latest>$time_tok.latest$</latest>
        </search>
        <option name="charting.chart">line</option>
        <option name="charting.axisLabelsX.majorLabelStyle.overflowMode">ellipsisNone</option>
        <option name="charting.axisLabelsX.majorLabelStyle.rotation">0</option>
        <option name="charting.axisTitleX.visibility">visible</option>
        <option name="charting.axisTitleY.visibility">visible</option>
        <option name="charting.axisTitleY2.visibility">visible</option>
        <option name="charting.axisX.scale">linear</option>
        <option name="charting.axisY.scale">linear</option>
        <option name="charting.axisY2.enabled">0</option>
        <option name="charting.axisY2.scale">inherit</option>
        <option name="charting.chart.bubbleMaximumSize">50</option>
        <option name="charting.chart.bubbleMinimumSize">10</option>
        <option name="charting.chart.bubbleSizeBy">area</option>
        <option name="charting.chart.nullValueMode">connect</option>
        <option name="charting.chart.showDataLabels">none</option>
        <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
        <option name="charting.chart.stackMode">default</option>
        <option name="charting.chart.style">shiny</option>
        <option name="charting.drilldown">all</option>
        <option name="charting.layout.splitSeries">0</option>
        <option name="charting.layout.splitSeries.allowIndependentYRanges">0</option>
        <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
        <option name="charting.legend.placement">right</option>
      </chart>
    </panel>
    <panel>
      <title>Tx</title>
      <chart>
        <title>Mbps</title>
        <search>
          <query>type=switch sourcetype=server_stats subtype=$switch_name$ instance=$instance_name$ plugin= interface JobTagName=$JobTagName$ | stats values(eth_outbytes) as valueTx by instance, _time |streamstats current=f window=1 last(valueTx) as oldTx last(_time) as oldtime by instance | eval total = (valueTx-oldTx)/(_time-oldtime) | eval Tx_Rate=(total*8)/1000000 | timechart avg(Tx_Rate) as "Tx (Mbps)" by instance</query>
          <earliest>$time_tok.earliest$</earliest>
          <latest>$time_tok.latest$</latest>
        </search>
        <option name="charting.chart">line</option>
        <option name="charting.axisLabelsX.majorLabelStyle.overflowMode">ellipsisNone</option>
        <option name="charting.axisLabelsX.majorLabelStyle.rotation">0</option>
        <option name="charting.axisTitleX.visibility">visible</option>
        <option name="charting.axisTitleY.visibility">visible</option>
        <option name="charting.axisTitleY2.visibility">visible</option>
        <option name="charting.axisX.scale">linear</option>
        <option name="charting.axisY.scale">linear</option>
        <option name="charting.axisY2.enabled">0</option>
        <option name="charting.axisY2.scale">inherit</option>
        <option name="charting.chart.bubbleMaximumSize">50</option>
        <option name="charting.chart.bubbleMinimumSize">10</option>
        <option name="charting.chart.bubbleSizeBy">area</option>
        <option name="charting.chart.nullValueMode">connect</option>
        <option name="charting.chart.showDataLabels">none</option>
        <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
        <option name="charting.chart.stackMode">default</option>
        <option name="charting.chart.style">shiny</option>
        <option name="charting.drilldown">all</option>
        <option name="charting.layout.splitSeries">0</option>
        <option name="charting.layout.splitSeries.allowIndependentYRanges">0</option>
        <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
        <option name="charting.legend.placement">right</option>
      </chart>
    </panel>
  </row>

