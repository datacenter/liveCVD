    </panel>
    <panel>
      <title>Tx Error</title>
      <chart>
        <search> 
          <query>type=switch sourcetype=server_stats subtype=$switch_name$ instance=$instance_name$ plugin= interface JobTagName=$JobTagName$ | stats values(eth_outerr) as out_error by instance, _time | streamstats current=f window=1 last(out_error) as old_error| eval OUT_error = (out_error-old_error) | timechart avg(OUT_error) as "Tx Error" by instance</query>
          <earliest>$time_tok.earliest$</earliest>
